{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "dnd-test",
  "version": "0.1.0",
  "identifier": "com.dnd-test.app",
  "build": {
    "beforeDevCommand": "deno task dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "deno task build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "dnd-test",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all"
  }
}
